server {
    listen 80;
    server_name khr_domain www.khr_domain;
    root /var/www/khr_domain;

    index index.html index.htm index.php;

    location / {
        try_files $uri $uri/ =404;
    }

    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.2-fpm.sock;
     }

    location ~ /\.ht {
        deny all;
    }

}